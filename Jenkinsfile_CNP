#!groovy
//noinspection GroovyUnusedAssignment
@Library("Infrastructure") _

import uk.gov.hmcts.contino.Environment

Environment environment = new Environment(env)

onMaster {
  sharedInfrastructurePipeline('rpx', environment.nonProdName, 'nonprod')
  sharedInfrastructurePipeline('rpx', environment.demoName, 'demo')
  sharedInfrastructurePipeline('rpx', environment.prodName, 'prod')
}

onHMCTSDemo {
  sharedInfrastructurePipeline('rpx', environment.hmctsDemoName, 'hmctsdemo')
}
